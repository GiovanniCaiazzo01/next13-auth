// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  ucode String @unique
  username String
  email   String   @unique
  password String 
  created_at String
  edited_at String
  jwt_token String

}